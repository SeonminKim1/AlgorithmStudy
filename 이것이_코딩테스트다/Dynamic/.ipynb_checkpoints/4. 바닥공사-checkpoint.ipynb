    "n = 7\n",
    "\n",
    "d = [0] * 10\n",
    "\n",
    "# 다이나믹 bottom up\n",
    "d[1] = 1\n",
    "d[2] = 3\n",
    "for i in range(3, n+1):\n",
    "    d[i] = d[i-1] + d[i-2]*2\n",
    "\n",
    "print(d)\n",
    "print(d[n])"
