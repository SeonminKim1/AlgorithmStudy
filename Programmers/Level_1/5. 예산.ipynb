    "def solution(d, budget):\n",
    "    answer = 0\n",
    "        \n",
    "    for i in sorted(d):\n",
    "        if budget >= i:\n",
    "            budget -= i\n",
    "            answer+=1\n",
    "    return answer\n",
    "\n",
    "solution([1,3,2,5,4], 9)"
   ]
  }
